{
	"#!/usr/bin/env zsh": {
		"prefix": "!",
    "body": ["#!/usr/bin/env zsh"]
  },
	"#!/usr/bin/env bash": {
		"prefix": "!b",
    "body": ["#!/usr/bin/env bash"]
  },
	"print spearator line": {
		"prefix": "p",
    "body": ["echo \"-------------------------------\""]
  },
	"echo \"\"": {
		"prefix": "e",
    "body": ["echo ${1:\"$0\"}"]
  },
	"echo \"\"": {
		"prefix": "e2",
    "body": ["echo $($0)"]
  },
	"echo ${array[@]}": {
		"prefix": "ea",
    "body": ["echo ${${0:array}[@]}"]
  },
	"debug puts": {
		"prefix": "pd",
		"body": ["echo \"${1}: $${1}\""],
  },
	"debug puts2": {
		"prefix": "ppp",
		"body": ["echo \"${1}: $${1}\""],
  },
	"debug puts3": {
		"prefix": "pp",
		"body": [
			"echo '-------------------------------'",
			"echo \"${1}: $${1}\"",
			"echo '-------------------------------'"
		],
  },

	"disable shellcheck": {
		"prefix": "dis",
		"body": ["# shellcheck disable=${1:SC2154}"],
  },

	"if": {
		"prefix": "if",
		"body": [
			"if [ ${1:-n \"\\$ZSH_VERSION\"} ]; then",
			"  ${0}",
			"fi"
		],
  },
	"if else": {
		"prefix": "ife",
		"body": [
			"if [ ${1:-n \"\\$ZSH_VERSION\"} ]; then",
			"  ${0}",
			"else",
			"   ",
			"fi"
		],
  },
}
